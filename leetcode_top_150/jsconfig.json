{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Node",
        "lib": ["ES2022", "DOM"],
        "target": "ES2022",
        "allowImportingTsExtensions": true,
        "checkJs": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true
    },
    "exclude": ["node_modules", "**/node_modules/*"]
}
